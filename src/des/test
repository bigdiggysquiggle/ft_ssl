#include "ft_ssl.h"

int		d(void)
{
	t_ssl_input	*input;
	input = malloc(sizeof(t_ssl_input));
	char	*input2;
	unsigned long c = 0x23F5AE3D543B2D0B;
	input2 = malloc(sizeof(char) * 8);
	input->input = ft_memcpy(input2, &c, 8);
	input->len = 8;
	input->key = 0x1000000000000000;
	input->outfd = 1;
	input->flags = 16;
	ft_des_ecb(input);
	return (0);
}

int		e(void)
{
	t_ssl_input	*input;
	input = malloc(sizeof(t_ssl_input));
	input->input = ft_strdup("test");
	input->len = 4;
	input->key = 0x1000000000000000;
	input->outfd = 1;
	input->flags = 0;
	ft_printf("r: ");
	ft_des_ecb(input);
	ft_printf("\ne: 0C0D0E8C09540DCE\n");
	return (0);
}

int		main(int ac, char **av)
{
	if (ac > 1)
		return (d());
	return (e());
}
